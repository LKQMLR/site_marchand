<%- include('header-html') %>

<h2>Inscription</h2>

<form action="/inscription" method="post">

    <label for="name">prénom:</label>
    <input type="text" id="name" name="name" required tabindex="1" aria-label="saisir votre prénom">

    <label for="lastname">nom:</label>
    <input type="text" id="lastname" name="lastname" required tabindex="2" aria-label="saisir votre nom">

    <label for="email">email:</label>
    <input type="email" id="email" name="email" required tabindex="3" aria-label="saisir votre email">

    <label for="password">mot de passe:</label>
    <input type="text" id="password" name="password" required tabindex="4" aria-label="saisir votre mot de passe">
</form>


